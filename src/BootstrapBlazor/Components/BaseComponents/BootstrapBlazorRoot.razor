@inherits ComponentBase
@namespace BootstrapBlazor.Components

@RenderBody()

<Dialog />
<PopoverConfirm />
<SweetAlert />
<Message @ref="MessageContainer" />
<Toast @ref="ToastContainer" />
<Print />

<Download />
<FullScreen />
<Clipboard />
<Title />
<ResizeNotification />

@code {
    RenderFragment RenderChildContent =>
    @<CascadingValue Value="this" IsFixed="true">
        @ChildContent
    </CascadingValue>;
}
